{
  "name": "coin-counter",
  "version": "1.0.0",
  "description": "This is a simple coin counter which demonstrates how to use Signals in Vanilla JS application.",
  "main": "index.html",
  "license": "MIT",
  "scripts": {
    "clean:demo": "npx rimraf ../docs/coin-counter",
    "start": "parcel src/index.html --open",
    "build": "parcel build src/index.html",
    "build:demo": "yarn clean:demo && parcel build src/index.html --out-dir=../docs/coin-counter --public-url=./"
  },
  "dependencies": {
    "@preact/signals-core": "^1.3.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.5",
    "sass": "^1.63.6"
  }
}
